    "@types/jest": "^29.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.0",
    "@types/response-time": "^2.3.5",
