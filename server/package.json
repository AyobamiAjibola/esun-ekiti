  "description": "esun ekiti",
  "main": "./public/index.js",
  "scripts": {
    "lint": "eslint . --fix",
    "build": "tsc -b",
    "start:prod": "node ./public/index.js",
    "dev:debug": "nodemon --config nodemon.json --inspect src/modules/history/history.services.ts",
    "dist": "tsc -p .",
    "start:dev": "nodemon --config nodemon.json src/index.ts",
    "profile:dev": "node --prof src/modules/history/history.controller.ts",
    "create:db": "cross-env NODE_ENV=development npx sequelize-cli db:create --config src/sequelize/config/config.js",
    "migrate": "cross-env NODE_ENV=development npx sequelize-cli db:migrate --config src/sequelize/config/config.js",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate src/sequelize/config/config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
